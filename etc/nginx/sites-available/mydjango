server {
        listen 80;
        server_name www.mydjangp.jp;

        location = /favicon.ico {access_log off; log_not_found off;}
        location /static/ {
                root /home/ubuntu/mydjango;
        }

        location / {
                include proxy_params;
                proxy_pass http://unix:/home/ubuntu/mydjango/config/config.sock;
        }
}
